<iframe id="iframe" src="<%= url_for :raw => "true", :release_notes_format => params[:release_notes_format] %>" frameborder="0" scrolling="auto" style="width: 80%; height: 600px; border: 1px solid grey; border-radius: 3px;"> </iframe>

